{
    "manifest_version": 3,
    "type": "framework",
    "name": "LLAPI",
    "slug": "LLAPI",
    "description": "LiteLoaderQQNT的插件API",
    "version": "0.1.0",
    "thumbnail": "./icon.png",
    "author": [
        {
            "name": "Night-stars-1",
            "link": "https://github.com/Night-stars-1"
        }
    ],
    "repository": {
        "repo": "LiteLoaderQQNT-Plugin-LLAPI",
        "branch": "main",
        "use_release": {
            "tag": "latest",
            "name": "LiteLoaderQQNT-Plugin-LLAPI.zip"
        }
    },
    "platform": [
        "win32",
        "linux",
        "darwin"
    ],
    "injects": {
        "renderer": "./src/renderer.js",
        "main": "./src/main.js",
        "preload": "./src/preload.js"
    }
}